if guess == number:
    print("SAHIII JAMABBB")
else:
    print("SAMJHA CHAUHA PR YEH ULTAR GALAT HAI!! SAHI JANAB YEH THA:", number)
















  PIN = "9106"
balance = 20000

print("Welcome To MY Atm")

attempts = 0
while attempts < 3:
    user_pin = input("Enter your PIN: ")
    if user_pin == PIN:
        print("PIN acceted")
        break
    else:
        attempts += 1
        print(f"Invalid PIN. You have {3 - attempts} attempts remaining.")
else:
    print("Too many incorrect PIN attempts. Your card is temp. blocked. Please try again after 24 hours.")
while True:
    print("\n--- ATM Menu ---")
    print("1. Check Balance")
    print("2. Withdraw")
    print("3. Deposit")
    print("4. Exit")

    choice = input("Enter your choice (1-4): ")

    if choice == "1":
        print(f"Your balance is: Rs{balance}")
    elif choice == "2":
        amount = int(input("Enter amount to withdraw: Rs"))
        if amount <= balance:
            balance -= amount
            print(f"Withdrawal successful. New balance: Rs{balance}")
        else:
            print("Insufficient balance.")
    elif choice == "3":
        amount = int(input("Enter amount to deposit: Rs"))
        balance += amount
        print(f"Deposit successful. New balance: Rs{balance}")
    elif choice == "4":
        print("Thank you for using our ATM. Goodbye!")
        break
    else:
        print("Invalid choice. Please try again.")














num = float(input("Enter a number: "))

if num > 0:
    print("The number is Positive")
elif num < 0:
    print("The number is Negative")
else:
    print("The number is Zero")

if num % 2 == 0:
    print("The number is even")
else:
    print("The number is odd")









import smtplib
import ssl
from email.message import EmailMessage

EMAIL = "ayushassociates2006@gmail.com"
APP_PASSWORD = "your_app_password_here"
RECEIVER = "lakhyaassociates05@gmail.com"

msg = EmailMessage()
msg["From"] = EMAIL
msg["To"] = RECEIVER
msg["Subject"] = "Hiii"
msg.set_content("This side ayush")

context = ssl.create_default_context()

with smtplib.SMTP_SSL("smtp.gmail.com", 465, context=context) as server:
    server.login(EMAIL, APP_PASSWORD)
    server.send_message(msg)

print("done!")











